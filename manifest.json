{
  "manifest_version": 2,
  "name": "joblink",
  "description": "Gather all your personal links in one place for job applications.",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": ["css/bootstrap.min.css"],
      "js": ["js/bootstrap.min.js"]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}
